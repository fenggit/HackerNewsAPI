{
  "name": "hackernews-api",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "bin": {
    "wrangler": ".wrangler-deploy-wrapper.js",
    "wrangler-deploy": ".wrangler-deploy-wrapper.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "wrangler pages deploy .next --project-name=hackernews-api",
    "versions:upload": "wrangler pages versions upload .next --project-name=hackernews-api",
    "postinstall": "chmod +x .wrangler-deploy-wrapper.js .wrangler-versions-wrapper.js deploy.sh versions-upload.sh 2>/dev/null || true"
  },
  "dependencies": {
    "next": "^16.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0"
  }
}
